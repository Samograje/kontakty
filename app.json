{
  "expo": {
    "name": "Kontakty",
    "slug": "kontakty",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ],
      "splash": {
        "mdpi": "./assets/img/splash/splash-mdpi.png",
        "hdpi": "./assets/img/splash/splash-hdpi.png",
        "xhdpi": "./assets/img/splash/splash-xhdpi.png",
        "xxhdpi": "./assets/img/splash/splash-xxhdpi.png",
        "xxxhdpi": "./assets/img/splash/splash-xxxhdpi.png",
        "resizeMode": "cover",
        "backgroundColor": "#0068bf"
      }
    }
  }
}
